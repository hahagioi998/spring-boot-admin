{
  "name": "spring-boot-admin-server-ui",
  "private": true,
  "description": "Spring Boot Admin UI",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "build:dev": "vite build --emptyOutDir --sourcemap --mode development",
    "build:watch": "vite build --emptyOutDir --watch --sourcemap --mode development",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/vue-fontawesome": "3.0.0-5",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "ansi_up": "^5.1.0",
    "autolinker": "^3.15.0",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "d3-array": "^3.1.5",
    "d3-axis": "^3.0.0",
    "d3-brush": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-time": "^3.0.0",
    "event-source-polyfill": "1.0.26",
    "file-saver": "2.0.5",
    "iso8601-duration": "^1.3.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.2",
    "popper.js": "^1.16.1",
    "pretty-bytes": "^6.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^7.5.5",
    "v3-infinite-loading": "^1.0.6",
    "vue": "3.2.25",
    "vue-i18n": "9.1.9",
    "vue-router": "4.0.14",
    "vue3-click-away": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.1.28",
    "@storybook/vue3": "^6.4.21",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/user-event": "14.1.0",
    "@testing-library/vue": "6.5.1",
    "@vitejs/plugin-vue": "2.3.0",
    "@vue/vue3-jest": "26.0.1",
    "autoprefixer": "10.4.4",
    "babel-loader": "^8.2.4",
    "eslint": "^6.8.0",
    "eslint-plugin-storybook": "^0.5.8",
    "eslint-plugin-vue": "8.6.0",
    "jest-environment-jsdom": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "msw": "^0.39.2",
    "msw-storybook-addon": "^1.6.3",
    "postcss": "8.4.12",
    "tailwindcss": "3.0.23",
    "vite": "2.9.0",
    "vite-plugin-static-copy": "^0.4.4",
    "vue-loader": "^16.8.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/.jest/register-context.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/.jest/jest.setup.js"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(d3-.*|internmap)/)"
    ],
    "transform": {
      ".+\\.vue$": "@vue/vue3-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      ".+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/main/frontend/$1"
    },
    "testMatch": [
      "**/*.spec.js"
    ],
    "testURL": "https://example.com"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
